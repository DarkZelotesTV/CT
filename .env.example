# LiveKit API credentials (used by docker-compose.yml and server)
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# URL that clients use to connect to LiveKit (e.g., ws://host:7880)
LIVEKIT_PUBLIC_URL=ws://your-livekit-host:7880

# Optional: Admin URL if different from the public URL
# LIVEKIT_ADMIN_URL=http://your-livekit-host:7880
